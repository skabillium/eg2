{
  "name": "eg2",
  "version": "0.7.0",
  "description": "CLI for managing cloud secrets",
  "homepage": "https://github.com/skabillium/eg2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skabillium/eg2.git"
  },
  "bugs": {
    "url": "https://github.com/skabillium/eg2/issues"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "files": [
    "bin",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "environment",
    "secret",
    "ssm"
  ],
  "bin": {
    "eg2": "bin/cli.js"
