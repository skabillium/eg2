  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.490.0",
    "c12": "^1.6.1",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "picomatch": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.2",
    "@types/picomatch": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
